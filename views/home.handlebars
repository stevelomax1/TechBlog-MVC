<div class="posts">
  {{#each posts}}
  <div class="cardC">

    <div class="head-block alignment">
      <h3>
        <a href="/post/{{id}}">
          {{title}}
        </a>
      </h3>

      <div>

        Created by: {{user.username}} {{formatDate createdAt}}
      </div>
    </div>
    <div class="cardC-body">
      {{body}}
    </div>
  </div>
  {{/each}}
</div>